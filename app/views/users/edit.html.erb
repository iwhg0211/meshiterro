<h2>プロフィール編集</h2>
<%= form_with model: @user do |f| %>
<label for = "imputName">Name</laber>
<%= f.text_field :name, autofocus: true, id:"imputName", placeholder:"名前" %>
<label for = "imputImage">ProfileImage</label>
<%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
<%= f.submit "変更を保存" %>
<% end %>